{
  "name": "@use-compose/i18next-core",
  "version": "0.1.3",
  "private": false,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/arthur-plazanet/i18next-compose.git"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "test": "vitest"
  },
  "devDependencies": {
    "@use-compose/logger": "0.0.6",
    "typescript": "5.9.2",
    "vite-plugin-dts": "4.5.4",
    "vite": "7.1.2",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "i18next": "25.0.2",
    "i18next-fs-backend": "2.6.0",
    "happy-dom": "18.0.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
